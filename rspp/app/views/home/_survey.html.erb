<% if user_signed_in? %>
<%= javascript_tag do %>
window.currentUserId = '<%= current_user.id %>'
window.surveyContent = '<%=  escape_javascript @survey.to_json %>'
<% end %>
<% end %>
<div class="survey-title">
	<%= @survey.title %>
</div>
<div class="survey-content" data-controller="<%= survey_path(@survey) %>"></div>

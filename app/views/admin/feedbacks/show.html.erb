<div class="admin-feedback container">

  <div class="col-lg-6 user-name">
    <%= @feedback.name %>
  </div>

  <div class="col-lg-6 text-right email">
    <%= @feedback.email %>
  </div>

  <div class="col-lg-12 body">
    <% @feedback.body.split("\n").each do |paragraph| %>
      <%= paragraph %>
      <br>
    <% end %>
  </div>

  <div class="col-lg-12 back-link">
    <%= link_to "Назад", admin_feedbacks_path %>
  </div>
</div>

<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 partners-container">
  <div class="partners-content">
    <div class="partners-title">
      <h2>
        Наши партнёры
      </h2>
    </div>
    <div class="partners-carousel">
      <% 5.times do |i| %>
        <div class="single-partner">
          <%= image_tag "partner#{i + 1}.png" %>
        </div>
      <% end %>
    </div>
  </div>
</div>
